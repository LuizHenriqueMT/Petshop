  object QueryRelatorio: TFDQuery
    Connection = Conexao
    SQL.Strings = (
      'Select *, Cliente.Clie_Nome, Cliente.Clie_CPF'
      'FROM pet'
      'INNER Join Cliente '
      'ON Pet.Clie_Codigo = Cliente.Clie_Codigo '
      'WHERE pet.Clie_Codigo')
    Left = 336
    Top = 424
